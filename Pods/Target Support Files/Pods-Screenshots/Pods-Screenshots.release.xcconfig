FRAMEWORK_SEARCH_PATHS = $(PLATFORM_DIR)/Developer/Library/Frameworks $(inherited) "$(SDKROOT)/Developer/Library/Frameworks" "$(PLATFORM_DIR)/Developer/Library/Frameworks"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/KIF" "${PODS_ROOT}/Headers/Public/SimulatorStatusMagic" "${PODS_ROOT}/Headers/Public/TOCScreenshot"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/KIF" -isystem "${PODS_ROOT}/Headers/Public/SimulatorStatusMagic" -isystem "${PODS_ROOT}/Headers/Public/TOCScreenshot" -DKIF_XCTEST
OTHER_LDFLAGS = -ObjC -l"Pods-Screenshots-KIF" -l"Pods-Screenshots-SimulatorStatusMagic" -l"Pods-Screenshots-TOCScreenshot" -framework "CoreGraphics" -framework "UIKit" -framework "XCTest"
PODS_ROOT = ${SRCROOT}/Pods